  "license": "MIT",
  "version": "0.0.2",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "NODE_ENV=development run-p start:dev autobuild",
    "start": "sirv build --single",
    "start:dev": "NODE_ENV=development sirv build --single --dev",
    "stories": "NODE_ENV=development start-storybook",
    "build-storybook": "build-storybook -o build",
    "static-stories": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
